{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Товары",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "price_old": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    },
    "composition": {
      "type": "string"
    },
    "expiration_date": {
      "type": "string"
    },
    "period_storage": {
      "type": "string"
    },
    "danger_class": {
      "type": "string"
    },
    "destination": {
      "type": "richtext"
    },
    "application": {
      "type": "richtext"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "packing": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::packing.packing"
    },
    "packing_count": {
      "type": "integer"
    },
    "count_metric": {
      "type": "integer"
    },
    "metric": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::metric.metric"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "product_application": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-application.product-application",
      "inversedBy": "products"
    },
    "guide": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::guide.guide",
      "inversedBy": "products"
    },
    "favourites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::favourite.favourite",
      "mappedBy": "product"
    },
    "order_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-products.order-products",
      "mappedBy": "product"
    },
    "rest_products": {
      "type": "integer",
      "min": 0
    }
  }
}
